{
    "$schema": "https://github.com/fastfetch-cli/fastfetch/raw/dev/doc/json_schema.json",
   
    "modules": [
        "break",
        {
            "type": "custom",
            "format": "\u001b[90m┌──────────────────────Hardware──────────────────────┐"
        },
        {
            "type": "host",
            "key": " PC",
            "keyColor": "#81daaf"
        },
        {
            "type": "os",
            "key": "│ ├",
            "keyColor": "#81daaf"
        },
        {
            "type": "cpu",
            "key": "│ ├󰍛",
            "showPeCoreCount": true,
            "keyColor": "#81daaf"
        },
        {
            "type": "gpu",
            "key": "│ ├󰍛",
            "keyColor": "#81daaf"
        },
        {
            "type": "memory",
            "key": "│ ├󰍛",
            "keyColor": "#81daaf"
        },
        {
            "type": "disk",
            "key": "│ ├",
            "keyColor": "#81daaf"
        },
        {
            "type": "wm",
            "key": "└ └",
            "keyColor": "#81daaf"
        },
        {
            "type": "custom",
            "format": "\u001b[90m└────────────────────────────────────────────────────┘"
        },
    ]
}
